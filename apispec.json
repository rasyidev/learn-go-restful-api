{
    "openapi":"3.0.2",
    "info": {
      "title":"Category RESTful API",
      "description": "API Spec for Category RESTful API",
      "version":"1.0"
    },
    "servers": [
      {"url":"http://localhost:9090/api"}
    ],
    "paths": {
      "/categories": {
        "get":{
          "description": "List all categories",
          "responses": {
            "200": {
              "description": "Success get all categories", 
              "content": {
                "aplication/json" :{
                  "schema":{
                    "type": "object",
                    "properties": {
                      "code": {
                        "type": "number"
                      },
                      "status": {
                        "type": "string"
                      },
                      "data": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "id": {
                              "type": "number"
                            },
                            "name":{
                              "type": "string"
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }

        }
      }
    }
  }